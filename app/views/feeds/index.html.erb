<% @title = @site.name %>
<h3>Сайт: <%= @site.name %> <%= @site.url %> <%= link_to 'rss', site_feed_rss_path(@site, current_user.rsskey) %></h3>
<% @feeds.each do |feed| %>
  <p><%= link_to feed.title, site_feed_path(@site, feed) %> (<%= link_to feed.url, feed.url %>)</p>
  <p><%= link_to 'Добавить в избранное', favorit_add_path(feed_id: feed.id), method: :post %></p>
  <p><%= raw feed.description %></p>
  <hr>
<% end %>
<%= will_paginate @feeds %>
</ul>
